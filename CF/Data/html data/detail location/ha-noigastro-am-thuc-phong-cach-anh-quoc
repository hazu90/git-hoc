            $(function() {
                if (window.location.hash == "#map") {
                    loadMicroMap();
                }
                $('#map-report-error').click(function() {
                    angular.element('#microMap').scope().ShowReport();
                    angular.element('#microMap').scope().$apply();
                });
            })

            function loadMicroMap() {
                if (angular) {
                    angular.element('#microMap').scope().Show();
                    angular.element('#microMap').scope().$apply();
                }
            }
