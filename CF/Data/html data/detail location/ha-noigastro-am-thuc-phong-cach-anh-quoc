<script type="text/javascript">

    $(window).scroll(function () {
        if (window._micNavTime)
            clearTimeout(_micNavTime);
        window._micNavTime = setTimeout(function () {
            if (!window.microNavs) return;
            var wTop = $(window).scrollTop();
            for (var i = 0; i < microNavs.length; i++) {
                var top = microNavs[i].Block.offset().top;
                if (wTop + 300 > top) {
                    var nav = $('.mic-navigation li[data-item-id="' + microNavs[i].Id + '"]');
                    if (nav.length > 0) {
                        $('.mic-navigation li').removeClass('active');
                        nav.addClass('active');
                    }
                }
                //console.log(wTop + ':' + top);
            }
        }, 10)
    });
</script>




