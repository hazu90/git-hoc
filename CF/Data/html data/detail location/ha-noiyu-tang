<script type="text/javascript">
    var backUrl = undefined, loginSuccessCallback, refreshAlbumIntervalId;
    $(function () {
        $('[data-toggle="tooltip"]').tooltip();
        $('.upload').click(function () {
            //$(".search-div").hide();
            var dishId = $(this).attr('dishid');
            loginSuccessCallback = function() {
                LoadMultipleUploadPictureForm(dishId);
            };
            LoadMultipleUploadPictureForm(dishId);
            return false;
        });
        $('.rating').click(function (e) {
            //$(".search-div").hide();
            $('html').scrollTop(1600);
            return false;
        });

        $(".photoslider").click(function() {
            var id = '662072';
            _resSafetyCertificationModel.fnShow({ResId:id});
        });


        if('1'=='1'){
            $('#foody-hot-line').html('Điện thoại: 1900 6044 Email: info@foody.vn');
            $('#foody-hot-line').show();
        }
        //if(!window.IsHomeMicrosite){
        //    var top= $('#float-menu-container').position().top-$('#container-s').height()-10;
        //    $('body,html').animate({scrollTop : top},0);
        //}


    });

    function LoadMultipleUploadPictureForm(dishId) {

        angular.element('div[ng-controller="HelperCtrl"]').scope().ShowDlgLogin(function (arg) {
            if (arg.LoggedIn)
            {
                SetCustomerToken();
                OpenUploadPictureForm(dishId);
            }
        });

        //var d = new Date();
        //$.ajax({
        //    type: "GET",
        //    url: '/Review/IsLogin?' + d.getTime(),
        //    success: function (response) {
        //        if (response.success == true) {
        //            SetCustomerToken();
        //            OpenUploadPictureForm(dishId);
        //        }
        //        else {
        //            loginSuccessCallback = function () {
        //                $.modal.close();
        //                SetCustomerToken();
        //                OpenUploadPictureForm(dishId);
        //            };
        //            //$('#loginPopup').modal({ persist: true });
        //            LoginPopup.Show();
        //        }
        //    }
        //});
    }
    function SetCustomerToken()
    {
        if (window.CONST_MEDIA_XUSER_TOKEN==null || window.CONST_MEDIA_XUSER_TOKEN=="")
        {
            var d = new Date();
            $.ajax({
                type: "GET",
                url: '/Review/GetCustomerToken?' + d.getTime(),
                success: function (response) {
                    window.CONST_MEDIA_XUSER_TOKEN=response;
                }
            });
        }
    }
    function OpenUploadPictureForm(dishId){
        var d = new Date();
        var link = "/__get/Gallery/NewAlbumPopup?RestaurantID=662072&Name=Yu%20Tang";
        if (dishId)
            link += '&DishId=' + dishId;
        $('#reviewDialog').html('<div style="padding-top:100px; padding-bottom:100px; padding-left:400px;"><img alt="Đang tải ..." src="//static.foody.vn/s1/style/images/loading-review.gif" /></div>').modal();
        $('#reviewDialog').load(link + '&t=' + d.getTime(), function() {
            setupAlbumUpload();
        }).modal();
    }

</script>

<script type="text/javascript">
    window.ReviewLanguages = {
        Common_You_Have:'Bạn có',
        Common_Points:'điểm',
        Review_You_Lose:'Bạn bị trừ',
        Redeem_OrderStatus_Pending:'Chờ duyệt'
    };
    var loginSuccessCallback; //= LoadAddNewReviewForm;
    var isReviewPage = true;
    var refreshIntervalId;


    $('.reviewLink').live('click', function() {
        // $(".search-div").hide();
        isModifiedMode = false;
        //loginSuccessCallback = OpenWriteReviewForm;
        //LoadAddNewReviewForm();
        var resid = $(this).attr('resid');
        UploadReview.LoadAddNewReviewForm(resid);
        return false;
    });
    $(function() {
        // Dialog Link
        var url = window.location.href;
        if (url.search("#binh-luan-nhap") >= 0) {
            // $(".search-div").hide();
            isModifiedMode = false;
            //loginSuccessCallback = OpenWriteReviewForm;
            //LoadAddNewReviewForm();
            //var resid = $('#draf-review-load').attr('resid');
            //console.warn(resid);
            UploadReview.LoadAddNewReviewForm('662072');
        }
        bindTooltip();
        addCommentWatermark();
        bindWriteCommentAutoresize();
        currentTimeStamp = 1512611024;
        CalculatingReviewPostedTime();
    });


    function GetNewReview(reviewId) {
        $.post('/__post/Review/GetReviewEx',{id:reviewId},function (res) {
            if(!res)
            {  hideLoadingProcess();
                closeNewReviewPopup();
                return;
            }

            angular.element('div[ng-controller="ResReviewCtrl"]').scope().Insert(res);

            hideLoadingProcess();
            closeNewReviewPopup();
        });

        return;

        if (isReviewPage) {

            if (IsFiltering()) {
                resetFiltering(function() {
                    //ScrollToReview(reviewId);
                    //bindPostedTime(reviewId);
                    closeNewReviewPopup();
                });
                if (typeof(drawChart) == 'function')
                    drawChart();
            } else {
                //Indo redirect
                if ('False' === 'True') {
                    var reviewDirectlink = '/ha-noi/yu-tang' + '/' + window.routeConfig.microsite.reviewPath + '-' + reviewId;
                    window.location.href = reviewDirectlink;

                }
                var d = new Date();
                $.get('/Review/GetReviewById?t=' + d.getTime(), { reviewId: reviewId, template: 1 }, function(response) {
                    if (response.success) {
                        RemoveReviewByIndex(5);
                        if (typeof homeReviewsModel != 'undefined' && homeReviewsModel != null) {
                            homeReviewsModel.addNewReview(response.data);
                        }

                        if (typeof resReviewsModel != 'undefined' && resReviewsModel != null) {
                            resReviewsModel.addNewReview(response.data);
                        }
                        //$('#review-list').prepend('<div id="review-'+reviewId+'">'+html+'</div>');
                        if (typeof(updateReviewTabs) != 'undefined') {
                            $('#review-tab div.micro-home-recent-review.review-item.empty').remove();
                            updateReviewTabs($("ul.review-tabs li:first"));
                        }
                        removeUserTooltip(response.userid);
                        bindTooltip();
                        //loadUserTotalReviews(reviewId);
                        bindPostedTime(reviewId);
                        updateTotalUserReviewsAndPictures(reviewId);
                        addCommentWatermark();
                        ScrollToReview(reviewId);
                        bindReviewReportByReviewId(reviewId);
                        IncreaseGlobalTotalReview(1);
                        hideLoadingProcess();
                        closeNewReviewPopup();
                        if (typeof(drawChart) == 'function')
                            drawChart();
                    }
                });
            }

        } else {
            var reviewdetaillink = '/ha-noi/yu-tang' + '/' + routeConfig.microsite.reviewPath + '-' + reviewId;
            var d = new Date();
            $.ajax({
                type: "GET",
                url: '/__get/Review/CalculationRestaurantReviewSummary?restaurantId=662072&' + d.getTime(),
                success: function(response) {
                    if (response) {
                        window.location = reviewdetaillink;
                    }
                }
            });
        }
    }
    $(document).ready(function() {
        var a=$('#avatarVideo');
        var activeMenuId=$(".list-tool li.active").attr('data-item-id');
        if (a && activeMenuId && activeMenuId==0)
        {
            var videoTypeId=a.attr('data-video-type');
            if (videoTypeId==1)//youtube
                a.click();
        }
    });
</script>

